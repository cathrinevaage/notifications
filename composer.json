{
  "name": "netflex/notifications",
  "description": "Netflex Notifications Mail Driver for Laravel",
  "type": "library",
  "license": "MIT",
  "keywords": [],
  "config": {
    "platform": {
      "php": "7.3"
    },
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.3",
    "illuminate/mail": "^8.0",
    "illuminate/support": "^8.0",
    "mustache/mustache": "^2.13",
    "netflex/api": "^3.0",
    "netflex/foundation": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Netflex\\Notifications\\": "src/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Netflex\\Notifications\\Providers\\NotifcationsServiceProvider"
      ]
    }
  }
}
